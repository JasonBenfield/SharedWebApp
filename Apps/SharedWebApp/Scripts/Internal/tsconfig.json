{
  "compileOnSave": false,
  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "lib": [ "ES2020", "dom" ],
    "module": "commonjs",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "ES2020",
    "importHelpers": true,
    "typeRoots": [
      "../../node_modules/@types"
    ]
  },
  "include": [
    "./**/*.ts",
    "../Lib/**/*.d.ts"
, "../Lib/OData/Types.ts"  ],
  "exclude": [
    "Imports",
    "node_modules",
    "wwwroot"
  ]
}